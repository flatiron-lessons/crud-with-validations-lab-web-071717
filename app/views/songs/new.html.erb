<h1> This is the Song form </h1>

<!-- make sure to define @song in the controller -->
<%= form_for @song do |s| %>

	<%= s.label :title %><br>
	<%= s.text_field :title %><br>

	<%= s.label :artist_name %><br>
	<%= s.text_field :artist_name %><br>

	<%= s.label :genre %><br>
	<%= s.text_field :genre %><br>

	<%= s.label :release_year %><br>
	<%= s.text_field :release_year %><br>

	<%= s.label :released? %><br>
	<%= s.text_field :released %><br>
	

	<%= s.submit %>
<% end %>

